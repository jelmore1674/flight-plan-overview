{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "flight-plan-overview",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "process": {
        "all": false,
        "relaunch": true
      },
      "window": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.justinelmore.flight-plan-overview",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://releases.justinelmore.dev/flight-plan-overview/{{target}}/{{arch}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI0RDY3QjQxRjA2NTFCQTYKUldTbUcyWHdRWHZXSktCbFhBL3ppOGswRDI0bCs0YVp1WVltb3BRKzl1YzR4dFl0SG9qMnNISkgK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "flight-plan-overview",
        "maxWidth": 2160,
        "minWidth": 768,
        "maxHeight": 3880,
        "minHeight": 300
      }
    ]
  }
}
